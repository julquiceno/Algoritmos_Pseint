Algoritmo sumaDeDosNumeros
	
	//Definicion de las variables:
	Definir num1,num2,suma Como Numero;
	
	// Entradas:
	Escribir 'Digita el primer número: ';
	Leer num1;
	Escribir 'Digita el segundo número: ';
	Leer num2;
	
	// Proceso:
	suma <- num1+num2;
	
	// Salida:
	Escribir 'El resultado de la suma es: ',suma;
	
FinAlgoritmo
